{
  "profile": "django",
  "indent": 2,
  "max_line_length": 120,
  "max_attribute_length": 120,
  "format_attribute_template_tags": true,
  "format_css": false,
  "format_js": false,
  "preserve_blank_lines": true,
  "preserve_leading_space": false,
  "max_blank_lines": 2,
  "close_void_tags": false,
  "ignore": "H006,H030,H031",
  "exclude": ".venv,venv,env,node_modules,staticfiles",
  "extend_exclude": ".git",
  "linter_output_format": "{code} {line}:{col} {message} {match}",
  "custom_blocks": "{% partialdef %}",
  "custom_html": "",
  "blank_line_after_tag": "load,extends,include",
  "blank_line_before_tag": "",
  "line_break_after_multiline_tag": true,
  "no_line_after_yaml": false,
  "no_function_formatting": false,
  "no_set_formatting": false,
  "require_pragma": false
}